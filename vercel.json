{
  "version": 2,
  "builds": [
    {
      "src": "api/generate-pdf.js",
      "use": "@vercel/node",
      "config": {
        "installCommand": "npm install --legacy-peer-deps && npm rebuild && npm install uploadthing --force"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/generate-pdf",
      "dest": "api/generate-pdf.js"
    }
  ]
}